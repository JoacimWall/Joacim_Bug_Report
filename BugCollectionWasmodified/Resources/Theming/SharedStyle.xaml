    <!--  Margins  -->

    <OnPlatform x:Key="StoryViewCellMargin" x:TypeArguments="Thickness">
        <On Platform="iOS" Value="0,4,0,6" />
        <On Platform="Android" Value="0,4,0,6" />
    </OnPlatform>

    <!--  ActivityIndicator  -->
    <Style TargetType="ActivityIndicator">
        <Setter Property="WidthRequest" Value="100" />
        <Setter Property="HeightRequest" Value="100" />
        <Setter Property="Color" Value="{x:Static tm_theming:AppColors.Primary500Color}" />
        <Setter Property="VerticalOptions" Value="Center" />
        <Setter Property="HorizontalOptions" Value="Center" />
        <Setter Property="IsRunning" Value="{Binding IsLoading}" />
        <Setter Property="IsVisible" Value="{Binding IsLoading}" />
    </Style>

