    <!--  _____________________________ BORDER STYELS ___________________________________________________  -->
    <Style x:Key="StandardBorderStyle" TargetType="Border">
        <Setter Property="Stroke" Value="{x:Static tm_theming:AppColors.Gray100Color}" />
        <Setter Property="BackgroundColor" Value="{x:Static tm_theming:AppColors.WhiteColor}" />
        <Setter Property="StrokeThickness" Value="2" />
        <Setter Property="StrokeShape" Value="RoundRectangle 12,12,12,12" />
    </Style>
    <!--  _____________________________ FRAME STYELS ___________________________________________________  -->
    <Style x:Key="StandardFrameStyle" TargetType="Frame">
        <Setter Property="Margin" Value="0" />
        <Setter Property="HasShadow" Value="False" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="BorderColor" Value="{x:Static tm_theming:AppColors.Gray100Color}" />
        <Setter Property="BackgroundColor" Value="{x:Static tm_theming:AppColors.WhiteColor}" />
        <Setter Property="CornerRadius" Value="12" />
        <Setter Property="IsClippedToBounds">
            <OnPlatform x:TypeArguments="x:Boolean">
                <On Platform="iOS" Value="False" />
                <On Platform="Android" Value="True" />
            </OnPlatform>
        </Setter>

    </Style>

    <Style x:Key="NoShadowFrameStyle" TargetType="Frame">
        <Setter Property="Margin" Value="15" />
        <Setter Property="HasShadow" Value="False" />
        <Setter Property="Padding" Value="15" />
        <Setter Property="BorderColor" Value="{x:Static tm_theming:AppColors.Primary200Color}" />
        <Setter Property="BackgroundColor" Value="{x:Static tm_theming:AppColors.WhiteColor}" />
